<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".CreationCompleteActivity">


    <ImageView
        android:id="@+id/img_creation_complete"
        android:layout_width="220dp"
        android:layout_height="146dp"
        android:layout_marginTop="32dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/icon_register_complete" />

    <TextView
        android:id="@+id/text_creation_complete_subtitle"
        android:layout_width="171dp"
        android:layout_height="48dp"
        android:layout_marginTop="12dp"
        android:text="등록이 완료되었습니다\n반려하루를 시작해보세요"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/img_creation_complete" />

    <View
        android:id="@+id/divider1_creation_complete"
        android:layout_width="360dp"
        android:layout_height="2px"
        android:layout_marginTop="44dp"
        android:background="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/text_creation_complete_subtitle" />

    <TextView
        android:id="@+id/text_creation_complete_content1"
        android:layout_width="83dp"
        android:layout_height="28dp"
        android:layout_marginTop="20dp"
        android:text="가족 그룹명"
        android:textAlignment="textStart"
        android:textColor="#000000"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.079"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/divider1_creation_complete" />

    <TextView
        android:id="@+id/text_familyName"
        android:layout_width="83dp"
        android:layout_height="28dp"
        android:layout_marginTop="20dp"
        android:text="그룹명"
        android:textAlignment="textEnd"
        android:textColor="#000000"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.887"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/divider1_creation_complete" />

    <View
        android:id="@+id/divider1_creation_complete2"
        android:layout_width="360dp"
        android:layout_height="1px"
        android:layout_marginTop="68dp"
        android:background="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.509"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/divider1_creation_complete" />

    <TextView
        android:id="@+id/text_familyNickname"
        android:layout_width="83dp"
        android:layout_height="28dp"
        android:layout_marginTop="20dp"
        android:text="닉네임명"
        android:textAlignment="textEnd"
        android:textColor="#000000"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.887"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/divider1_creation_complete2" />

    <TextView
        android:id="@+id/text_creation_complete_content2"
        android:layout_width="90dp"
        android:layout_height="28dp"
        android:layout_marginTop="20dp"
        android:text="가족 내 닉네임"
        android:textAlignment="textStart"
        android:textColor="#000000"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.079"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/divider1_creation_complete2" />

    <View
        android:id="@+id/divider1_creation_complete3"
        android:layout_width="360dp"
        android:layout_height="1px"
        android:layout_marginTop="68dp"
        android:background="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.509"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/divider1_creation_complete2" />

    <TextView
        android:id="@+id/text_petName"
        android:layout_width="174dp"
        android:layout_height="29dp"
        android:layout_marginTop="20dp"
        android:text="반려동물 이름"
        android:textAlignment="textEnd"
        android:textColor="#000000"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.843"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/divider1_creation_complete3" />

    <TextView
        android:id="@+id/text_creation_complete_content3"
        android:layout_width="90dp"
        android:layout_height="28dp"
        android:layout_marginTop="20dp"
        android:text="반려동물"
        android:textAlignment="textStart"
        android:textColor="#000000"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.08"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/divider1_creation_complete3" />

    <View
        android:id="@+id/divider1_creation_complete4"
        android:layout_width="360dp"
        android:layout_height="1px"
        android:layout_marginTop="68dp"
        android:background="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/divider1_creation_complete3" />

    <TextView
        android:id="@+id/text_creation_complete_subtitle2"
        android:layout_width="205dp"
        android:layout_height="21dp"
        android:layout_marginTop="20dp"
        android:text="반려동물을 추가 등록 하고싶다면?"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.126"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/divider1_creation_complete4" />

    <android.widget.Button
        android:id="@+id/btn_add"
        android:layout_width="75dp"
        android:layout_height="25dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/grey_button"
        android:insetTop="0dp"
        android:insetBottom="0dp"
        android:shadowColor="@color/dark_grey"
        android:text="추가하기"
        android:textAlignment="center"
        android:textSize="10sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.761"
        app:layout_constraintStart_toEndOf="@+id/text_creation_complete_subtitle2"
        app:layout_constraintTop_toBottomOf="@+id/divider1_creation_complete4" />

    <android.widget.Button
        android:id="@+id/btn_start"
        android:layout_width="320dp"
        android:layout_height="60dp"
        android:layout_marginTop="100dp"
        android:background="@drawable/blue_button"
        android:insetTop="0dp"
        android:insetBottom="0dp"
        android:shadowColor="@color/dark_grey"
        android:text="케어 시작하기"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/divider1_creation_complete4" />

    <android.widget.Button
        android:id="@+id/btn_invite"
        android:layout_width="320dp"
        android:layout_height="60dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/whiteblue_click_button"
        android:insetTop="0dp"
        android:insetBottom="0dp"
        android:shadowColor="@color/dark_grey"
        android:text="그룹 초대하기"
        android:textAlignment="center"
        android:textColor="@color/main_blue"
        android:textSize="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_start" />

    <TextView
        android:id="@+id/text_inviteComment"
        android:layout_width="313dp"
        android:layout_height="27dp"
        android:layout_marginTop="12dp"
        android:text="@string/invite_comment"
        android:textAlignment="center"
        android:textColor="@color/main_blue"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_invite" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="757dp" />

</androidx.constraintlayout.widget.ConstraintLayout>