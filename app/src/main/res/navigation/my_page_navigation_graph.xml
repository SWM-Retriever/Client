<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/my_page_navigation_graph"
    app:startDestination="@id/myPageMainFragment">

    <fragment
        android:id="@+id/myPageMainFragment"
        android:name="org.retriever.dailypet.ui.mypage.MyPageMainFragment"
        android:label="MyPageMainFragment" >
        <action
            android:id="@+id/action_myPageMainFragment_to_webViewActivity"
            app:destination="@id/webViewActivity" />
        <action
            android:id="@+id/action_myPageMainFragment_to_myPagePetFragment"
            app:destination="@id/myPagePetFragment" />
    </fragment>
    <activity
        android:id="@+id/webViewActivity"
        android:name="org.retriever.dailypet.ui.mypage.WebViewActivity"
        android:label="WebViewActivity" >
        <argument
            android:name="url"
            app:argType="string" />
    </activity>
    <fragment
        android:id="@+id/myPagePetFragment"
        android:name="org.retriever.dailypet.ui.mypage.MyPagePetFragment"
        android:label="MyPagePetFragment" >
        <action
            android:id="@+id/action_myPagePetFragment_to_myPagePetDetailFragment"
            app:destination="@id/myPagePetDetailFragment" />
        <action
            android:id="@+id/action_myPagePetFragment_to_createPetFragment2"
            app:destination="@id/createPetFragment2" />
    </fragment>
    <fragment
        android:id="@+id/myPagePetDetailFragment"
        android:name="org.retriever.dailypet.ui.mypage.MyPagePetDetailFragment"
        android:label="MyPagePetDetailFragment" >
        <argument
            android:name="petDetailItem"
            app:argType="org.retriever.dailypet.model.mypage.PetDetailItem" />
        <action
            android:id="@+id/action_myPagePetDetailFragment_to_createPetFragment2"
            app:destination="@id/createPetFragment2" />
    </fragment>
    <fragment
        android:id="@+id/createPetFragment2"
        android:name="org.retriever.dailypet.ui.signup.CreatePetFragment"
        android:label="CreatePetFragment" >
        <argument
            android:name="isFromMyPage"
            app:argType="boolean" />
    </fragment>
</navigation>